 </types>

 <fields>
 
   <field name="fqn_full" type="string" 
			indexed="true" stored="true" required="true" 
			omitNorms="true" />
			
   <field name="fqn" type="FT_fqn" indexed="true" stored="false" required="true" /> 
   
   <field name="sname_contents" type="FT_sname_contents" 
			indexed="true" stored="false" required="true" 
			omitNorms="true" />
	   
	<field name="fqn_contents" type="FT_fqn_contents" 
			indexed="true" stored="false" required="true"
			omitNorms="true" />
	
	<field name="entity_id" type="long" 
			indexed="true" stored="true" required="true"
			omitNorms="true" /> 
   
	<field name="entity_type" type="string" 
			indexed="true" stored="true" required="true"
			omitNorms="true" termVector="true"/>
  
     <!-- use fields -->
  
       		
	<field name="jdkLib_use_fqn_contents" type="FT_fqn_contents" 
			indexed="true" stored="false" required="false"
			multiValued="true"
			omitNorms="true" />
	
   <field name="jdkLib_use_sname_contents" type="FT_sname_contents" 
			indexed="true" stored="false" required="false" 
			multiValued="true"
			omitNorms="true" />
	
			
	<field name="jdkLib_use_javadoc" type="FT_text" 
		indexed="true" stored="false" required="false"
		multiValued="true" omitNorms="true" />
	
		
	<!--  field name="lib_use_comment" type="FT_text" 
		indexed="true" stored="false" required="false"
		multiValued="true" omitNorms="true" / -->

	<!-- for facets -->
	
	<field name="jdkLib_use_fqn_full" type="FT_use_fqn_full" 
			indexed="true" stored="true" required="false" 
			multiValued="true" termVector="true"
			omitNorms="true" />
			
   
    <!-- mlt fields snames are cc split -->
    
    <field name="sim_sname_contents_via_jdkLib_use" type ="FT_sim_sname_contents"
	    indexed="true" stored="true" required="false"
	    multiValued="true"
		omitNorms="true" />
	
	<!-- tanimoto coefficient similarity -->	
	<field name="simTC_sname_contents_via_jdkLib_use" type ="FT_sim_sname_contents"
	    indexed="true" stored="true" required="false"
	    multiValued="true"
		omitNorms="true" />
	
	<!-- hamming distance similarity -->	
	<field name="simHD_sname_contents_via_jdkLib_use" type ="FT_sim_sname_contents"
	    indexed="true" stored="true" required="false"
	    multiValued="true"
		omitNorms="true" />
		
	<field name="sim_sname_contents_via_wordnet" type ="FT_sim_sname_contents_wn"
	    indexed="true" stored="true" required="false"
	    multiValued="true"
		omitNorms="true" />	
		
	<!--   field name="matched_used_jars" type ="FT_text"
	    indexed="true" stored="true" required="false" multiValued="true"
		omitNorms="true" / -->	

    <!-- full text -->
    <field name="full_text" type="FT_text" 
		indexed="true" stored="false" required="false"
		omitNorms="true" />
	
	<!--  stemmed fields -->
	
	<field name="sname_contents_stemmed" type="FT_sname_contents_stemmed" 
			indexed="true" stored="false" required="true" 
			omitNorms="true" />	
	<field name="fqn_contents_stemmed" type="FT_fqn_contents_stemmed" 
			indexed="true" stored="false" required="true"
			omitNorms="true" />		
	<field name="jdkLib_use_fqn_contents_stemmed" type="FT_fqn_contents_stemmed" 
			indexed="true" stored="false" required="false"
			multiValued="true"
			omitNorms="true" />		 
	
	<field name="jdkLib_use_sname_contents_stemmed" type="FT_sname_contents_stemmed" 
			indexed="true" stored="false" required="false" 
			multiValued="true"
			omitNorms="true" />		
	
	<field name="jdkLib_use_javadoc_stemmed" type="FT_text_stemmed" 
		indexed="true" stored="false" required="false"
		multiValued="true" omitNorms="true" />	
	
	<field name="sim_sname_contents_via_jdkLib_use_stemmed" type ="FT_sim_sname_contents_stemmed"
	    indexed="true" stored="true" required="false"
	    multiValued="true"
		omitNorms="true" />
	
	<!-- tanimoto coefficient similarity -->	
	<field name="simTC_sname_contents_via_jdkLib_use_stemmed" type ="FT_sim_sname_contents_stemmed"
	    indexed="true" stored="true" required="false"
	    multiValued="true"
		omitNorms="true" />
	
	<!-- hamming distance similarity -->	
	<field name="simHD_sname_contents_via_jdkLib_use_stemmed" type ="FT_sim_sname_contents_stemmed"
	    indexed="true" stored="true" required="false"
	    multiValued="true"
		omitNorms="true" />
	
	<field name="sim_sname_contents_via_wordnet_stemmed" type ="FT_sim_sname_contents_wn_stemmed"
	    indexed="true" stored="true" required="false"
	    multiValued="true"
		omitNorms="true" />	
		
	<field name="sim_sname_contents_stemmed" type ="FT_sim_sname_contents_stemmed"
	    indexed="true" stored="true" required="false"
		omitNorms="true" multiValued="true"/>
	
	<field name="full_text_stemmed" type="FT_text_stemmed" 
		indexed="true" stored="false" required="false"
		omitNorms="true" />	
	
	
	
	<!--  end stemmed fields -->
	    
 </fields>

 <!-- Field to use to determine and enforce document uniqueness. 
      Unless this field is marked with required="false", it will be a required field
   -->
 <uniqueKey>entity_id</uniqueKey>

 <!-- SolrQueryParser configuration: defaultOperator="AND|OR" -->
 <solrQueryParser defaultOperator="AND"/>


<copyField source="sname_contents" dest="sname_contents_stemmed"/>
<copyField source="sname_contents" dest="sim_sname_contents_via_wordnet_stemmed"/>
<copyField source="sname_contents" dest="sim_sname_contents_via_wordnet"/>

<copyField source="fqn_contents" dest="fqn_contents_stemmed"/>
<copyField source="jdkLib_use_sname_contents" dest="jdkLib_use_sname_contents_stemmed"/>
<copyField source="jdkLib_use_fqn_contents" dest="jdkLib_use_fqn_contents_stemmed"/>
<copyField source="jdkLib_use_javadoc" dest="jdkLib_use_javadoc_stemmed"/>
<copyField source="sim_sname_contents_via_jdkLib_use" dest="sim_sname_contents_via_jdkLib_use_stemmed"/>
<copyField source="simTC_sname_contents_via_jdkLib_use" dest="simTC_sname_contents_via_jdkLib_use_stemmed"/>
<copyField source="simHD_sname_contents_via_jdkLib_use" dest="simHD_sname_contents_via_jdkLib_use_stemmed"/>
<copyField source="full_text" dest="full_text_stemmed"/>



 
  <!-- field for the QueryParser to use when an explicit fieldname is absent 
  		TODO change to full_text
  -->
  <defaultSearchField>full_text</defaultSearchField>

