   <fieldType name="FT_use_fqn_full" class="solr.TextField" positionIncrementGap="100">
	<analyzer type="index">
	<tokenizer
		class="org.apache.solr.analysis.KeywordTokenizerFactory"/>
	<filter class="solr.StopFilterFactory" words="stopwords-uses.txt" 
		ignoreCase="true"/>
      </analyzer>
      <analyzer type="query">
	<tokenizer
		class="org.apache.solr.analysis.KeywordTokenizerFactory"/>      
	<filter class="solr.StopFilterFactory" words="stopwords-uses.txt" 
		ignoreCase="true"/>
	
      </analyzer>
